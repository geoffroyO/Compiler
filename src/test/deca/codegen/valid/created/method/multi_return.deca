// Description:
//      Methode avec plusieurs return
// Resultats:
//      0
//      10
//      20

class A {
    int value(float x)
    {
        if (x < 0)
        {
            return(0);
        }

        if (x < 10)
        {
            return(10);
        }
        return(20);

    }
}

{
    A a = new A();
    int value;
    value = a.value(-7.9);
    println(value);
    value = a.value(5.0);
    println(value);
    value = a.value(50.0);
    println(value);
}